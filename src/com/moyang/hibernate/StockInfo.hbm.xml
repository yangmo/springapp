<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
    <class name="com.moyang.hibernate.StockInfo" table="stock_info">
        <id name = "stockId" type="string" column="stock_id" />
        <property name="stockName" column="stock_name" type="string" />
        <property name="marketplace" column="marketplace" type="string" />
    </class>

</hibernate-mapping>